@model GRP.Infrastructure.Models.Masters.ProfileInfoCategoryDTO

<div class="row">
    <div class="offset-md-3 col-md-6">
        <div class="x_panel">
            <div class="x_title">
                <h2>Category <small style="color:#17a2b8">Add Category below</small></h2>
                <ul class="nav navbar-right panel_toolbox">
                    <li><a href="javascript:void(0)" onclick="ListPartialView()">List Of Category</a></li>
                </ul>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <br />
                <form class="form-label-left input_mask" method="post" id="form">
                    <div class="form-group row">
                        <label class="col-form-label col-md-3 col-sm-3 ">CategoryName</label>
                        <div class="col-md-9 col-sm-9 ">
                            <input type="text" class="form-control" id="CategoryName" name="CategoryName" value="@(Model==null ? "" : Model.CategoryName==null ? "" : Model.CategoryName)">
                        </div>
                    </div>


                    <div class="ln_solid"></div>
                    <div class="form-group row">
                        <div class="col-md-9 col-sm-9  offset-md-3">
                            <input type="hidden" name="ProfileInfoCategoryId" value="@(Model == null ? 0  : Model.ProfileInfoCategoryId==null ? 0 : Model.ProfileInfoCategoryId)" />
                            <button type="button" class="btn btn-primary" onclick="ListPartialView()">Cancel</button>
                            <button class="btn btn-primary" type="reset" onclick="AddPartialView()">Reset</button>
                            <button type="button" class="btn btn-success" onclick="Add()">Submit</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>