                                                {
                                                    var projectPid = Model.Project?.Pid ?? Model.ProjectDetailList?.FirstOrDefault()?.PID ?? string.Empty;
                                                    var projectName = Model.Project?.Pname ?? Model.ProjectDetailList?.FirstOrDefault()?.PName ?? string.Empty;
                                                    var srLabel = string.IsNullOrWhiteSpace(projectPid)
                                                        ? projectName
                                                        : (string.IsNullOrWhiteSpace(projectName) ? projectPid : $"{projectPid} - {projectName}");

                                                    foreach (var item in Model.SupplierDetailsList)
                                                    {
                                                        var unreadCount = item.UnreadCount ?? 0;
                                                        var lastMessageTimestamp = item.LastMessageUtc?.ToString("o") ?? string.Empty;
                                                        var supplierId = item.SupplierId?.ToString() ?? string.Empty;
                                                        var country = string.IsNullOrWhiteSpace(item.Country)
                                                            ? string.Empty
                                                            : System.Text.RegularExpressions.Regex.Replace(item.Country, "\\s+", " ")
                                                                .Trim();
                                                        <tr>
