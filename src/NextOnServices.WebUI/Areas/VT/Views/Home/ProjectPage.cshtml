               function deleteMappedCountry(countryId, projectUrlId, mode) {
            var modeText = mode === 'exclude' ? 'excluded' : 'included';
            Swal.fire({
                title: 'Are you sure?',
                text: "This country will be removed from the " + modeText + " list.",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, remove it!'
            }).then((result) => {
                if (result.isConfirmed) {
                    BlockUI();
                    $.ajax({
                        type: "POST",
                        url: '/VT/Home/DeleteMappingCountries',
                        data: JSON.stringify({ ProjectURLID: projectUrlId, CountryID: countryId }),
                        contentType: "application/json",
                        dataType: "json",
                        success: function (response) {
                            UnblockUI();
                            Swal.fire({
                                icon: 'success',
                                title: 'Removed!',
                                text: 'Country removed successfully.',
                                confirmButtonColor: '#3085d6',
                                timer: 1500,
                                showConfirmButton: false
                            });
                            // Refresh the view after a short delay
                            setTimeout(function() {
                                MappedCountriesView();
                            }, 300);
                        },
                        error: function (error) {
                            UnblockUI();
                            Swal.fire({
                                icon: 'error',
                                title: 'Error',
                                text: 'Failed to remove country. Please try again.',
                                confirmButtonColor: '#d33'
                            });
                            console.error("Error deleting mapped country: ", error);
                        }
                    });
                }
            });
        }

