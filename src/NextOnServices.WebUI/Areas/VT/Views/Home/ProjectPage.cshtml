        function MappedCountriesView() {
            $('#tblSurveySpecs').find('tbody').find('tr').each(function () {
                var row = $(this);
                var id = row.find('td[data-itemid]').data('itemid'); 
                var displayHtml = '';

                if (id) {
                    GetMappedCountries(id).done(function (data) {
                        // Handle both direct response and wrapped response
                        var response = data.value || data || {};
                        var mode = response.mode || response.Mode || 'include';
                        var selectedCountries = response.selectedCountries || response.SelectedCountries || [];

                        if (selectedCountries && selectedCountries.length > 0) {
                            // Create badges for each country with remove option
                            var countryBadges = '';
                            selectedCountries.forEach(function(item) {
                                var countryId = item.countryId || item.CountryId || 0;
                                var countryName = item.countryName || item.CountryName || '';
                                if (countryName) {
                                    countryBadges += '<label class="B_grnd" style="display: inline-block!important; color: black; background-color: #e6f3ff; padding: 2px 5px; margin: 2px; border-radius: 3px; border: 1px solid #b3d9ff;">' +
                                        countryName +
                                        '<i class="fa fa-times" style="margin-left: 5px; cursor: pointer; color: #0066cc;" onclick="deleteMappedCountry(\'' + countryId + '\', \'' + id + '\', \'' + mode + '\')" title="Remove"></i>' +
                                        '</label>';
                                }
                            });
                            
                            // Add the message text
                            var messageText = '';
                            if (mode === 'exclude') {
                                messageText = '<span style="color: #666; font-style: italic; margin-left: 5px;">all countries except the selected ones.</span>';
                            } else {
                                messageText = '<span style="color: #666; font-style: italic; margin-left: 5px;">only for the countries chosen.</span>';
                            }
                            
                            displayHtml = countryBadges + messageText;
                        } else {
                            displayHtml = '<span style="color:#888;">No countries mapped.</span>';
                        }
                       
                        row.find('td[data-itemid]').html(displayHtml);
                    }).fail(function (error) {
                        console.error("Error fetching mapped countries: ", error);
                        row.find('td[data-itemid]').html('');
                    });
                }
            });
        }

