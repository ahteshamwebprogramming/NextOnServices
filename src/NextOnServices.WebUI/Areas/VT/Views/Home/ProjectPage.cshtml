         function LoadCountries() {
            var $countrySelect = $("#mulddlCountries");

            // avoid duplicate select2 instances
            if ($countrySelect.data('select2')) {
                $countrySelect.select2('destroy');
            }

            return $.ajax({
                type: "POST",
                url: '/VT/Home/GetCountries'
            }).done(function (data) {
                if (data && data.length > 0) {
                    let optionsHtml = "";

                    for (var i = 0; i < data.length; i++) {
                        optionsHtml += `<option value="${data[i].countryId}">${data[i].country}</option>`;
                    }

                    $countrySelect.html(optionsHtml);
                } else {
                    console.warn("No countries returned from server.");
                }

                $countrySelect.select2({
                    placeholder: "Select Country/Countries",
                    allowClear: true,
                    width: '100%'
                });
            }).fail(function (result) {
                console.error("Error fetching countries:", result);
            });
        }

