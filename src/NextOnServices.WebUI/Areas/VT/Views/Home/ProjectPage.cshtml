        function prefillMappedCountries(id) {
            GetMappedCountries(id).done(function (data) {
                var response = data.value || data || {};
                var mode = response.mode || response.Mode || 'include';
                var selectedCountries = response.selectedCountries || response.SelectedCountries || [];

                $('#rbInclude').prop('checked', mode === 'include');
                $('#rbExclude').prop('checked', mode === 'exclude');
                updateModeHelpText();

                if (selectedCountries && selectedCountries.length > 0) {
                    var selectedIds = selectedCountries
                        .map(function (item) {
                            return (item.countryId || item.CountryId || item.countryid || item.Countryid || item.countryID || item.CountryID || '').toString();
                        })
                        .filter(function (val) { return val !== ''; });

                    if ($("#mulddlCountries").data('select2')) {
                        $("#mulddlCountries").val(selectedIds).trigger('change');
                    } else {
                        $("#mulddlCountries").val(selectedIds);
                    }
                }
            }).fail(function (error) {
                console.error("Error pre-filling mapped countries:", error);
            });
        }

