                                                                        var displayCount = unreadCount > 99 ? "99+" : unreadCount.ToString();
                                                                        var srText = unreadCount == 1 ? "1 unread message" : $"{unreadCount} unread messages";
                                                                        <span class="chat-unread" data-unread-badge>
                                                                            <span class="chat-unread__count">@displayCount</span>
                                                                            <span class="sr-only">@srText</span>
                                                                        </span>
                                                                    }
                                                                </button>
                                                            </td>

                                                        </tr>
                                                    }
                                                }
                                            }
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
