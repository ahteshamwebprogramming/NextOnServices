<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100 p-l-50 p-r-50 p-t-77 p-b-30">
                <form class="login100-form validate-form" method="post" action="/VT/Supplier/ForgetPassword">
                    <span class="login100-form-title p-b-55">
                        Forget Password
                    </span>
                    <div class="m-b-16 wrap-input100">
                        <div class="wrap-input100 validate-input">
                            <input class="input100" type="text" placeholder="Supplier Code" asp-for="SupplierCode">
                            <span class="focus-input100"></span>
                            <span class="symbol-input100">
                                <span class="lnr lnr-user"></span>
                            </span>
                        </div>
                        @Html.ValidationMessageFor(model => model.SupplierCode, "", new { @class = "text-danger" })
                    </div>
                    <div class="container-login100-form-btn p-t-25">
                        <button class="login100-form-btn">
                            Submit
                        </button>
                    </div>
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    @*  <div class="text-center w-full p-t-42 p-b-22">
                    <span class="txt1">
                    Or login with
                    </span>
                    </div> *@
                    <div class="text-center w-full p-t-115" style="padding-top:40px;">
                        <span class="txt1">
                            Want to login?
                        </span>
                        <a class="txt1 bo1 hov1" href="/VT/Supplier/Login">
                            Click Here
                        </a>
                    </div>
                </form>
            </div>
        </div>
    </div>




