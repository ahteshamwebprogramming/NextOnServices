    <meta name="robots" content="noindex, follow">

    <style>
        .text-danger {
            color: red;
        }

            .text-danger ul {
                list-style: none;
                font-size: 13px;
            }
    </style>
</head>
<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100 p-l-50 p-r-50 p-t-77 p-b-30">
                <form class="login100-form validate-form" method="post" action="/VT/Supplier/Login">
                    <span class="login100-form-title p-b-55">
                        Login
                    </span>

                    <div class="wrap-input100 validate-input m-b-16" data-validate="Valid email is required: ex@abc.xyz">
                        <input class="input100" type="text"  asp-for="UserName" placeholder="">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <span class="lnr lnr-envelope"></span>
                        </span>
                    </div>

                    <div class="wrap-input100 validate-input m-b-16" data-validate="Password is required">
                        <input class="input100" type="password" placeholder="Password" asp-for="Password">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <span class="lnr lnr-lock"></span>
                        </span>
                    </div>

                    <div class="contact100-form-checkbox m-l-4">
                        <input class="input-checkbox100" id="ckb1" type="checkbox" name="remember-me">
                        <label class="label-checkbox100" for="ckb1">
                            Remember me
                        </label>
                    </div>

                    <div class="container-login100-form-btn p-t-25">
                        <button type="submit" class="login100-form-btn">
                            Login
                        </button>
                    </div>
                    @Html.ValidationSummary("", new { @class = "text-danger" })
                    @*  <div class="text-center w-full p-t-42 p-b-22">
                    <span class="txt1">
                    Or login with
                    </span>
                    </div> *@
                    <div class="text-center w-full p-t-115" style="padding-top:40px;">
                        <span class="txt1">
                            Forget Password?
                        </span>
                        <a class="txt1 bo1 hov1" href="/VT/Supplier/ForgetPassword">
                            Click Here
                        </a>
                    </div>
                </form>
            </div>
        </div>
    </div>




    <!--===============================================================================================-->
    @* <script src="vendor/jquery/jquery-3.2.1.min.js"></script> *@
    <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
